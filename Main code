void setup() {
    //Serial Monitor setup and connect to WiFi
    Serial.begin(115200);
    WiFi.begin(ssid, password);
    while (WiFi.status() != WL_CONNECTED) {
        delay(1000);
        Serial.println("Connecting to WiFi...");
    }
    Serial.println("Connected to WiFi");
    Serial.println("Enter text and press Enter to send:");
  pinMode(TOUCH_TAIL_PIN, INPUT_PULLUP);
  pinMode(TOUCH_HEAD_PIN, INPUT);
  pinMode(TOUCH_LEFT_FACE_PIN, INPUT);
  pinMode(TOUCH_RIGHT_FACE_PIN, INPUT);
  // Initialize the OLED display
  u8g2.begin();
  u8g2.enableUTF8Print();
  Serial.begin(115200);
  startTime = millis();
  timerRunning = true;
}

void loop() {
    if (WiFi.status() == WL_CONNECTED && Serial.available() > 0) {
        userInput = Serial.readStringUntil('\n');

        HTTPClient http;
        http.begin(serverName);
        http.addHeader("Content-Type", "application/json");
        http.addHeader("Authorization", "Bearer " + String(apiKey));

        StaticJsonDocument<200> doc;
        doc["prompt"] = userInput;
        doc["max_tokens"] = 60;
        String requestBody;
        serializeJson(doc, requestBody);

        int httpResponseCode = http.POST(requestBody);

        if (httpResponseCode > 0) {
            String response = http.getString();

            //print text
            StaticJsonDocument<600> responseDoc;
            deserializeJson(responseDoc, response);
            String textResponse = responseDoc["choices"][0]["text"].as<String>();
            Serial.println("Extracted text:");
            Serial.println(textResponse);
        } else {
            Serial.print("Error on sending request: ");
            Serial.println(httpResponseCode);
        }

        http.end();
        Serial.println("Enter next text and press Enter to send:");
    }
  static bool inTimerMode = false;  // Flag to check if in timer mode
  static bool tailPressedLastCycle = false;
  bool touchHead = digitalRead(TOUCH_HEAD_PIN);
  bool touchTail = !digitalRead(TOUCH_TAIL_PIN); // Assuming active LOW for tail touch sensor
  bool touchRightFace = digitalRead(TOUCH_RIGHT_FACE_PIN);
  bool touchLeftFace = digitalRead(TOUCH_LEFT_FACE_PIN);
  
  if (touchTail && !tailPressedLastCycle) {
        inTimerMode = !inTimerMode;
        tailPressedLastCycle = true;
        if (inTimerMode) {
            startTime = millis();
            timerRunning = true;
            showStartMessage = true; // Reset to show start message
        } else {
            timerRunning = false;
        }
    } else if (!touchTail) {
        tailPressedLastCycle = false;
    }
  if (showStartMessage && inTimerMode) {
        displayStartMessage();
        delay(1000); // Display for 1 second
        showStartMessage = false;
        startTime = millis(); // Reset timer after showing start message
    } else if (inTimerMode) {
        unsigned long currentTime = millis();
        unsigned long elapsedTime = currentTime - startTime;
        unsigned long timerDuration = focusPeriod ? focusDuration : restDuration;
       
        if (elapsedTime >= timerDuration) {
            focusPeriod = !focusPeriod; // Switch between focus and rest periods
            startTime = currentTime; // Reset the timer
            showStartMessage = focusPeriod; // Show start message only at the beginning of focus period
        }

        if (timerRunning) {
            if (focusPeriod) {
                showRemainingTime(timerDuration - elapsedTime);
            } else {
                displayRestMessage();
            }
        }
    } else {  if (touchHead) {
      animateGraph();
    } else if (touchLeftFace) {
      animateGraph2();
    } else if (touchRightFace) {
      animateGraph3();
    } else {
      u8g2.clearBuffer();
      u8g2.drawXBMP(0, 0, 128, 64, frame1_bits);
      u8g2.sendBuffer();}
    }
}
    
    
void displayStartMessage() {
    u8g2.clearBuffer();
    u8g2.setFont(u8g2_font_ncenB14_tr);
    u8g2.drawStr(10, 30, "Start");
    u8g2.drawStr(10, 50, "Meowmodoro");
    u8g2.sendBuffer();
}

void displayRestMessage() {
    u8g2.clearBuffer();
    u8g2.setFont(u8g2_font_ncenB14_tr);
    u8g2.drawStr(10, 30, "Rest Time!");
    u8g2.sendBuffer();
}

    
void animateGraph() {
  for (int frame = 1; frame <= 4; frame++) { // Forward loop from frame 1 to 6
    drawFrame(frame);
    delay(200); // Delay between frames
  }
  for (int frame = 3; frame >= 1; frame--) { // Reverse loop from frame 5 to 1
    drawFrame(frame);
    delay(200); // Delay between frames
  }
}


void animateGraph2() {
  for (int frame = 5; frame <= 8; frame++) { // Forward loop from frame 1 to 6
    drawFrame(frame);
    delay(200); // Delay between frames
  }
  for (int frame = 7; frame >= 5; frame--) { // Reverse loop from frame 5 to 1
    drawFrame(frame);
    delay(200); // Delay between frames
  }
}


void animateGraph3() {
  for (int frame = 9; frame <= 12; frame++) { // Forward loop from frame 1 to 6
    drawFrame(frame);
    delay(200); // Delay between frames
  }
  for (int frame = 11; frame >= 9; frame--) { // Reverse loop from frame 5 to 1
    drawFrame(frame);
    delay(200); // Delay between frames
  }
}

void showRemainingTime(unsigned long remainingTime) {
    if (!timerRunning) {
        return;
    }

    if (remainingTime > (focusPeriod ? focusDuration : restDuration)) {
        remainingTime = 0;
        timerRunning = false; // Stop the timer if it goes beyond the duration
    }

    // Convert to minutes
    unsigned long remainingMinutes = remainingTime / 60000;

  // Clear the entire screen
  u8g2.clearBuffer();
  u8g2.setFont(u8g2_font_ncenB14_tr);

  // Prepare the text to be displayed
  char buffer[10];
  sprintf(buffer, "%lu mins", remainingMinutes);

  // Draw the text on the screen
  u8g2.drawStr(10, 30, buffer); // Adjust the position as needed

  // Update the display with the new content
  u8g2.sendBuffer();
}




void drawFrame(int frameNumber) {
  u8g2.firstPage();
  do {
    switch (frameNumber) {
      case 1:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame1_bits);
        break;
      case 2:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame2_bits);
        break;
      case 3:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame3_bits);
        break;
      case 4:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame4_bits);
        break;
      case 5:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame5_bits);
        break;
      case 6:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame6_bits);
        break;
      case 7:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame7_bits);
        break;
      case 8:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame8_bits);
        break;
      case 9:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame9_bits);
        break;
      case 10:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame10_bits);
        break;
      case 11:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame11_bits);
        break;
      case 12:
        u8g2.drawXBM(0, 0, frame_width, frame_height, frame12_bits);
        break;
    }
  } while (u8g2.nextPage());
}
